{
   "background": {
      "page": "window.html",
      "css": "style.css"
   },
   "browser_action": {
      "default_icon": "icons/19.png",
      "default_popup": "window.html",
      "default_title": "Announcements"
   },
   "permissions": ["background", "storage", "alarms"],
   "description": "Publish announcements from a spreadsheet",
   "icons": {
      "128": "icons/128.png",
      "19": "icons/19.png",
      "48": "icons/48.png"
   },
   "manifest_version": 2,
   "name": "Demo Daily Announcements",
   "version": "0.1"
}
